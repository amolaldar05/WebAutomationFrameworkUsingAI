<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="MySuite" parallel="classes" thread-count="5">
    <listeners>
        <listener class-name="org.Utils.TestListener"/>
        <listener class-name="org.Utils.RetryListener"/>
    </listeners>
    <test name="MyTests">
        <classes>
            <!-- List your test classes here -->
            <class name="org.amazon.LoginTests"/>
            <class name="org.amazon.RegistrationTests"/>
        </classes>
    </test>
</suite>